{
  "expo": {
    "name": "ParkHere",
    "slug": "ParkHere",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/parkingicon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android":{
      "package": "com.dodeja.park_here",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/parking.png",
        "backgroundColor": "#ffffff"

      },
      "permissions":["ACCESS_FINE_LOCATION"],
      "config": {
  
        "googleMaps": {
          
          "apiKey": ""
        }
      }
    }
  }
}